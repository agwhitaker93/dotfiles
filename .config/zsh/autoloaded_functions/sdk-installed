sdk=$1
version=$2
provider=$3
if [ ${sdk} = 'java' ]; then
    result=$(sdk ls ${sdk} | grep ${version:-''} | grep ${provider:-''} | grep installed | grep -o '\S*-'${provider:-'\S*'})
else
    result=$(sdk ls ${sdk} | grep ${version:-''} | grep -o '\*\ \S*' | grep -o '\S*$')
fi
if [ -t 1 ]; then
    echo ${result}
else
    echo ${result} | head -n 1
fi
