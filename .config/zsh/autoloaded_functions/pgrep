function exclusive_grep_string() {
    first=$(echo $1 | cut -c1)
    rest=$(echo $1 | cut -c2-)
    echo "[${first}]${rest}"
}

if [[ "$#" -lt 2 ]]; then
    /bin/ps | /usr/bin/grep $(exclusive_grep_string $1)
else
    /bin/ps $1 | head -n 1 ; /bin/ps $1 | /usr/bin/grep $(exclusive_grep_string $2)
fi

